{
    "manifest_version": 2,
    "name": "CodeText",
    "version": "1.0.0",
    "description": "A Chrome extension that changes the stylings of code blocks on pages you visit",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "codetext.js"
            ]
        }
    ],
    "options_page": "options/index.html",
    "permissions": [
        "storage"
    ],

    "icons":{
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png",
        "256": "icons/256.png",
        "512": "icons/512.png"
    }
}